[*.{jsx,ts,tsx,vue}]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
ij_html_do_not_indent_children_of_tags = script
ij_typescript_force_semicolon_style = true
ij_typescript_enforce_trailing_comma = remove


[*.js]
indent_size = 2
ij_javascript_spaces_within_object_type_braces = false
